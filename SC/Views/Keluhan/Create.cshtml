@model SC.Models.Keluhan

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Keluhan</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="User.UserName" class="control-label"></label>
                <input asp-for="User.UserName" class="form-control" />
                <span asp-validation-for="User.UserName"  class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="User.ProfileMahasiswa.Prodi" class="control-label"></label>
                <input asp-for="User.ProfileMahasiswa.Prodi" class="form-control" />
                <span asp-validation-for="User.ProfileMahasiswa.Prodi" class="text-danger"></span>
            </div>
            <div class=" form-group">
                    <label asp-for="TanggalKeluhan" class="control-label"></label>
                    <input asp-for="TanggalKeluhan" type="date" class="form-control" />
                    <span asp-validation-for="TanggalKeluhan" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="KeluhanMahasiswa" class="control-label"></label>
                <input asp-for="KeluhanMahasiswa" class="form-control" />
                <span asp-validation-for="KeluhanMahasiswa" class="text-danger"></span>
            </div>
            <div class=" form-group">
                    <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
